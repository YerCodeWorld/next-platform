{
  "version": 2,
  "buildCommand": "pnpm build:vercel",
  "outputDirectory": "apps/web-next/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/web-next",
  "env": {
    "NEXT_PUBLIC_GOOGLE_CLIENT_ID": "@google_client_id",
    "DATABASE_URL": "@database_url",
    "NEXTAUTH_SECRET": "@nextauth_secret"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_GOOGLE_CLIENT_ID": "@google_client_id",
      "DATABASE_URL": "@database_url",
      "NEXTAUTH_SECRET": "@nextauth_secret"
    }
  },
  "functions": {
    "apps/web-next/app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/es",
      "permanent": false
    }
  ]
}