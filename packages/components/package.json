{
  "name": "@repo/components",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "",
  "exports": {
    ".": "./src/index.ts",
    "./src/*": "./src/*.ts",
    "./styles": "./dist/styles.css",
    "./globals.scss": "./src/globals.scss",
    "./font.css": "./src/font.css"
  },
  "scripts": {
    "build": "pnpm run build:scss && pnpm run build:types",
    "build:scss": "sass assets/sass/main.scss:dist/styles.css --style=compressed --source-map --load-path=node_modules",
    "build:types": "tsc --noEmit",
    "dev": "pnpm run watch:scss",
    "watch": "pnpm run watch:scss",
    "watch:scss": "sass assets/sass/main.scss:dist/styles.css --watch --style=expanded --source-map --load-path=node_modules",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@repo/api-bridge": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "eslint": "^9.22.0",
    "lightgallery": "^2.8.2",
    "lucide-react": "^0.515.0",
    "next-link": "^2.0.0",
    "next-navigation": "^1.0.6",
    "phosphor-react": "^1.4.1",
    "rc-slider": "^10.6.2",
    "react": "^19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.0",
    "react-fast-marquee": "^1.6.5",
    "react-modal-video": "^2.0.2",
    "react-scroll-to-top": "^3.0.0",
    "react-slick": "^0.30.3",
    "react-slider": "^2.0.6",
    "select2": "^4.1.0-rc.0",
    "slick-carousel": "^1.8.1",
    "sonner": "^2.0.5",
    "swiper": "^11.0.5",
    "typescript": "5.8.2",
    "vanilla-tilt": "^1.8.0",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@types/react-countup": "^4.3.1",
    "@types/react-visibility-sensor": "^5.1.0",
    "sass": "^1.80.6"
  }
}